<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>
<?import presentacion.*?>

<GridPane alignment="CENTER" hgap="10.0" vgap="10.0" xmlns:fx="http://javafx.com/fxml" fx:controller="presentacion.FXMLTableViewController">
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
  <Label alignment="CENTER" style="-fx-font: NORMAL 20 Tahoma;" text="Aditivos" GridPane.columnIndex="0" GridPane.rowIndex="0" />
  <TableView fx:id="tableView" GridPane.columnIndex="0" GridPane.rowIndex="1">
    <columns>
      <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="81.0" text="Id" fx:id="firstNameColumn">
        <cellFactory>
          <FormattedTableCellFactory alignment="CENTER" />
        </cellFactory>
        <cellValueFactory>
          <PropertyValueFactory property="id" />
        </cellValueFactory>
      </TableColumn>
      <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="169.0" text="Nombre">
        <cellValueFactory>
          <PropertyValueFactory property="nombre" />
        </cellValueFactory>
      </TableColumn>
    </columns>
    <items>
      <FXCollections fx:factory="observableArrayList" />
    </items>
    <sortOrder>
      <fx:reference source="firstNameColumn" />
    </sortOrder>
  </TableView>
  <HBox alignment="BOTTOM_RIGHT" spacing="10.0" GridPane.columnIndex="0" GridPane.rowIndex="2">
    <Button onAction="#addPerson" text="AÃ±adir Aditivo" />
  </HBox>
</GridPane>
